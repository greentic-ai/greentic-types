{
  "schema_version": "pack-v1",
  "pack_id": "vendor.provider.pack",
  "version": "1.0.0",
  "kind": "provider",
  "publisher": "vendor",
  "extensions": {
    "greentic.provider-extension.v1": {
      "kind": "greentic.provider-extension.v1",
      "version": "1.0.0",
      "digest": "sha256:abc123",
      "location": "https://example.com/provider-ext.json",
      "inline": {
        "providers": [
          {
            "provider_type": "vendor.search",
            "capabilities": ["messaging"],
            "ops": ["index", "query"],
            "config_schema_ref": "file://schemas/provider-config.json",
            "runtime": {
              "component_ref": "vendor.providers.search",
              "export": "greentic_provider",
              "world": "greentic:provider/runtime"
            },
            "docs_ref": "docs/provider.md"
          }
        ],
        "notes": "supports embedded providers"
      }
    }
  }
}
